        

<h2>Campaign Gallery</h2>
<div class="row projects">
  <div id="loader">
      <div class="loader-icon"></div>
  </div>
  <div class="col-md-12" id="portfolio-list">
    <!-- PORTFOLIO ITEMS-->
    <ul class="cbp-rfgrid">

    <% @campaigns.each do | campaign | %>

        <!-- PROJECT -->  
        <li class="wow fadeInLeft animated animated" data-wow-offset="30" data-wow-duration="1.5s" data-wow-delay="0.15s" style="visibility: visible;-webkit-animation-duration: 1.5s; -moz-animation-duration: 1.5s; animation-duration: 1.5s;-webkit-animation-delay: 0.15s; -moz-animation-delay: 0.15s; animation-delay: 0.15s;">
        <a href="/campaigns/<%=campaign.id%>" class="more"><img class="campaign-photo img-responsive" src="<%=campaign.photo_url%>" alt="<%=campaign.description%>">
      <div class="project-info">
        <div class="project-details">
          <h5 class="white-text red-border-bottom">
           <%=campaign.title%></h5>
          <div class="details white-text">
            Coveted By: <%=campaign.receiver.name%>
          </div>
        </div>
      </div>
      </a>
        </li>
        <!-- / PROJECT -->

    <% end %>


                <!-- ... -->
    </ul>
  </div>
  <!-- END OF PORTFOLIO ITEMS-->
</div>
        
       
